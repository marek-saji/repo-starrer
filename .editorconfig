; This file is for unifying the coding style for different editors and IDEs.
; More information at http://editorconfig.org

root = true

[*]
indent_style = space
indent_size = 4
tab_width = 4
quote_type = single
curly_bracket_next_line = true
indent_brace_style = Allman
end_of_line = lf
charset = utf-8
